<!-- Use this to loop through for different line options -->
<template_data>
    <category>
        <name>lines</name>
        <subcategory>
            <name>basic</name>
            <item>
                <description>Line Options</description>
                <type>loop_line_options</type>
                <data>
                    <item>
                        <variable>$display_name</variable>
                        <default_value>{$displayname.line.{$count}}</default_value>
                        <description>Display Name</description>
                        <type>input</type>
                    </item>
                    <item>
                        <type>break</type>
                    </item>
                    <item>
                        <variable>$sip_server</variable>
                        <default_value>{$server.ip.1}</default_value>
                        <description>SIP Server</description>
                        <max_chars>16</max_chars>
                        <type>input</type>
                    </item>
                    <item>
                        <variable>$sip_server_port</variable>
                        <default_value>{$server.port.1}</default_value>
                        <description>SIP Server Port</description>
                        <max_chars>6</max_chars>
                        <type>input</type>
                    </item>
                    <item>
                        <type>break</type>
                    </item>
                    <item>
                        <variable>$bk_sip_server</variable>
                        <default_value>{$server.ip.2}</default_value>
                        <description>SIP Server</description>
                        <max_chars>16</max_chars>
                        <type>input</type>
                    </item>
                    <item>
                        <variable>$bk_sip_server_port</variable>
                        <default_value>{$server.port.2}</default_value>
                        <description>SIP Server Port</description>
                        <max_chars>6</max_chars>
                        <type>input</type>
                    </item>
                    <item>
                        <type>break</type>
                    </item>
                    <item>
                        <variable>$enable_outbound_proxy_server</variable>
                        <default_value>0</default_value>
                        <description>Enable Outbound Proxy</description>
                        <type>list</type>
                        <data>
                            <text>Disabled</text>
                            <value>0</value>
                        </data>
                        <data>
                            <text>Enabled</text>
                            <value>1</value>
                        </data>
                    </item>
                    <item>
                        <variable>$transport</variable>
                        <default_value>0</default_value>
                        <description>Transport</description>
                        <type>list</type>
                        <data>
                            <text>UDP</text>
                            <value>0</value>
                        </data>
                        <data>
                            <text>TCP</text>
                            <value>1</value>
                        </data>
                        <data>
                            <text>TLS</text>
                            <value>2</value>
                        </data>
                    </item>
                    <item>
                        <variable>$nat_Traversal</variable>
                        <default_value>1</default_value>
                        <description>NAT Traversal</description>
                        <type>list</type>
                        <data>
                            <text>Disabled</text>
                            <value>0</value>
                        </data>
                        <data>
                            <text>STUN</text>
                            <value>1</value>
                        </data>
                    </item>
                    <item>
                        <variable>$stun_server</variable>
                        <default_value></default_value>
                        <description>STUN Server</description>
                        <max_chars>16</max_chars>
                        <type>input</type>
                    </item>
                    <item>
                        <variable>$stun_server_port</variable>
                        <default_value></default_value>
                        <description>STUN Server Port</description>
                        <max_chars>6</max_chars>
                        <type>input</type>
                    </item>
                    <item>
                        <variable>$voicemail_number</variable>
                        <default_value>*97</default_value>
                        <description>Voicemail Number</description>
                        <type>input</type>
                    </item>
                    <item>
                        <variable>$missed_call_log</variable>
                        <default_value>1</default_value>
                        <description>Missed Call Log</description>
                        <type>list</type>
                        <data>
                            <text>Disabled</text>
                            <value>0</value>
                        </data>
                        <data>
                            <text>Enabled</text>
                            <value>1</value>
                        </data>
                    </item>
                    <item>
                        <variable>$auto_answer</variable>
                        <default_value>0</default_value>
                        <description>Auto Answer</description>
                        <type>list</type>
                        <data>
                            <text>Disabled</text>
                            <value>0</value>
                        </data>
                        <data>
                            <text>Enabled</text>
                            <value>1</value>
                        </data>
                    </item>
                </data>
            </item>
        </subcategory>
    </category>
</template_data>